{
    "logging": {
        "sink": {}
    },
    "admin": {
        "disabled": true
    },
    "apps": {
        "http": {
            "servers": {
                "myserver": {
                    "routes": [
                        {
                            "handle": [
                                {
                                    "handler": "file_server",
                                    "browse": {
                                        "template_file": "file.html"
                                    }
                                }
                            ],
                            "terminal": true,
                            "match": [
                                {
                                    "host": [
                                        "localhost"
                                    ],
                                    "file": {
                                        "root": ""
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        }
    }
}