{
    "logging": {
        "sink": {
            "writer": {
                "output": "file",
                "filename": ""
            }
        },
        "logs": {
            "mylog": {
                "writer": {
                    "output": "file",
                    "filename": ""
                },
                "encoder": {
                    "format": "json",
                    "message_key": ""
                },
                "sampling": {
                    "first": 0
                }
            },
            "another": {
                "writer": {
                    "output": "net",
                    "address": ""
                },
                "encoder": {
                    "format": "logfmt",
                    "time_format": ""
                }
            }
        }
    },
    "admin": {
        "disabled": true,
        "config": {
            "persist": true
        },
        "origins": []
    },
    "apps": {
        "http": {
            "servers": {
                "myserver": {
                    "routes": [
                        {
                            "handle": [
                                {
                                    "handler": "file_server",
                                    "browse": {
                                        "template_file": "file.html"
                                    }
                                }
                            ],
                            "terminal": true,
                            "match": [
                                {
                                    "host": [
                                        "localhost"
                                    ],
                                    "file": {
                                        "root": ""
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        }
    }
}